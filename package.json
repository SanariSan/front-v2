    "lint-quiet": "./node_modules/.bin/eslint --quiet --ext .ts,.tsx,.js,.jsx .",
    "lint-fix": "./node_modules/.bin/eslint --fix --ext .ts,.tsx,.js,.jsx ."
  },
  "dependencies": {
    "@dbeining/react-atom": "^4.1.21",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "history": "^4.0.0",
    "isomorphic-fetch": "^3.0.0",
    "js-base64": "^3.7.2",
    "mime-types": "^2.1.35",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.1",
    "react-contexify": "^5.0.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^4.0.0",
