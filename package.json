    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^16.0.0",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^17.0.43",
    "@types/react-bootstrap": "^0.32.29",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/semantic-ui": "^2.2.7",
    "prettier": "^2.3.1",
    "react-app-rewired": "^2.2.1",
    "typescript": "<4.5.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
